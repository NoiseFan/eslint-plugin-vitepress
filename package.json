{
  "name": "eslint-plugin-vitepress",
  "version": "0.0.0",
  "packageManager": "pnpm@10.21.0",
  "description": "My awesome typescript library",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/NoiseFan/eslint-plugin-vitepress#readme",
  "bugs": {
    "url": "https://github.com/NoiseFan/eslint-plugin-vitepress/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:NoiseFan/eslint-plugin-vitepress.git"
  },
  "author": "noisefan <noisefan@163.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "@antfu/eslint-config": "^6.2.0",
    "eslint-plugin-format": "^1.0.2"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
